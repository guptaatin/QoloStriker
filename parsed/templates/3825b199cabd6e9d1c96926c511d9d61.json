{"components/products/carousel_2":"<div class=\"slide-home container\">\n    <h2 class=\"title-section\">{{lang 'home.heading_categories'}}</h2>\n    <section class=\"home-product-carousel\"\n        data-slick='{\n            \"infinite\": true,\n            \"dots\": true,\n            \"mobileFirst\": true,\n            \"slidesToShow\": 2,\n            \"slidesToScroll\": 1,\n            \"slide\": \"[data-product-slide]\",\n            \"responsive\": [\n                {\n                    \"breakpoint\": 1000,\n                    \"settings\": {\n                        \"slidesToShow\": 4,\n                        \"slidesToScroll\": 1\n                    }\n                },\n                {\n                    \"breakpoint\": 550,\n                    \"settings\": {\n                        \"slidesToShow\": 3,\n                        \"slidesToScroll\": 1\n                    }\n                }\n            ]\n        }'\n    >\n        {{#each categories}}\n        <div data-product-slide class=\"productCarousel-slide\">\n            <article class=\"card\">\n                <figure class=\"card-figure\">\n                    <a href=\"{{url}}\"\n                    class=\"card-figure__link\"\n                    aria-label=\"{{name}}\"\n                    >\n                        <div class=\"card-img-container\">\n                            <img src=\"\" alt=\"\">\n                        </div>\n                    </a>\n                </figure>\n                <div class=\"card-body\">\n                    <h3 class=\"card-title\">\n                        <a aria-label=\"{{name}}\" href=\"{{url}}\">\n                            {{name}}\n                        </a>\n                    </h3>\n                </div>\n            </article>\n        </div>\n        {{/each}}\n    </section>\n</div>\n","components/products/slider-shop":"{{> components/products/carousel_2 products=products list=\"New Products\"}}\n"}
