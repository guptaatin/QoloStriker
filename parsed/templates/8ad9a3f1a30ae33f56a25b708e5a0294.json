{"components/common/footer":"{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n\n{{/if}}\n<footer class=\"footer\" role=\"contentinfo\">\n    {{#if settings.show_newsletter_box}}\n        {{> components/common/subscription-form}}\n    {{/if}}\n    <div class=\"footer__column section-container\">\n        <section class=\"footer-info\">\n            <article class=\"footer-info-content\" data-section-type=\"footer-categories\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.column_title_2'}}</h3>\n                {{{region name=\"footer_column_widget1--global\"}}}\n            </article>\n            <article class=\"footer-info-content\" data-section-type=\"footer-brands\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.column_title_3'}}</h3>\n                {{{region name=\"footer_column_widget2--global\"}}}\n            </article>\n            <article class=\"footer-info-content\" data-section-type=\"storeInfo\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.column_title_4'}}</h3>\n                <div class=\"footer-info-list\">\n                    <div class=\"footer-contact__map\">\n                        <div>\n                            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                                <path d=\"M15.5312 11.3125L12.0312 9.8125C11.9375 9.78125 11.8438 9.75 11.7188 9.75C11.5 9.75 11.2812 9.875 11.1562 10.0312L9.59375 11.9375C7.15625 10.7812 5.21875 8.84375 4.0625 6.40625L5.96875 4.84375C6.125 4.71875 6.25 4.5 6.25 4.25C6.25 4.15625 6.21875 4.0625 6.1875 3.96875L4.6875 0.46875C4.5625 0.1875 4.28125 0 3.96875 0C3.9375 0 3.875 0.03125 3.8125 0.03125L0.5625 0.78125C0.21875 0.875 0 1.15625 0 1.5C0 9.53125 6.46875 16 14.5 16C14.8438 16 15.125 15.7812 15.2188 15.4375L15.9688 12.1875C15.9688 12.125 15.9688 12.0625 15.9688 12.0312C15.9688 11.7188 15.7812 11.4375 15.5312 11.3125Z\" fill=\"white\"/>\n                            </svg>\n                            <p>\n                                <a href=\"tel:{{lang 'footer.call_us_2'}}\">{{lang 'footer.call_us_2'}}</a>\n                            </p>\n                        </div>\n                    </div>\n                    <a class=\"customer-service\"\n                    href=\"mailto:{{lang 'footer.email'}}\"\n                    aria-label=\"Customer Service\"\n                    >\n                    <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                        <path d=\"M8 0C3.5 0 0.125 3.75 0 8V9.6875C0 10.9688 1 12 2.28125 12H2.84375C3.46875 12 4 11.5 4 10.8438V6.6875C4 6.03125 3.46875 5.5 2.84375 5.5H2.28125C2.15625 5.5 2.0625 5.53125 1.96875 5.5625C2.9375 3.1875 5.28125 1.53125 8 1.53125C10.6875 1.53125 13.0312 3.1875 14 5.53125C13.9062 5.53125 13.8125 5.5 13.6875 5.5H13.125C12.5 5.5 12 6.03125 12 6.6875V10.8438C12 11.5 12.5 12 13.125 12H13.6875C13.9688 12 14.25 11.9688 14.5 11.875V12.5C14.5 13.2188 13.9375 13.75 13.25 13.75H9.78125C9.53125 13.3125 9.03125 13 8.5 13H7.5C6.65625 13 6 13.6875 6 14.5C6 15.3438 6.65625 16 7.46875 16H8.5C9.03125 16 9.53125 15.7188 9.78125 15.25H13.25C14.75 15.25 16 14.0312 16 12.5V8C15.8438 3.75 12.4688 0 8 0ZM2.28125 7H2.5V10.5H2.28125C1.84375 10.5 1.5 10.125 1.5 9.6875V8C1.5 7.9375 1.5 7.875 1.5 7.78125C1.53125 7.34375 1.84375 7 2.28125 7ZM13.6875 10.5H13.5V7H13.6875C14.125 7 14.4375 7.375 14.4688 7.78125C14.4688 7.875 14.5 7.9375 14.5 8V9.6875C14.5 10.125 14.125 10.5 13.6875 10.5Z\" fill=\"white\"/>\n                    </svg>{{lang 'footer.btn_customer_service'}}\n                    </a>\n                </div>\n            </article>\n        </section>\n        {{#if settings.paypal_commerce_credit_message}}\n        <div class=\"footer-copyright\">\n            <p class=\"paypal-credit\">{{nl2br settings.paypal_commerce_credit_message}}</p>\n        </div>\n        {{/if}}\n        <div class=\"footer-bottom\">\n            {{#if theme_settings.show_copyright_footer}}\n                <div class=\"footer-copyright\">\n                    <p class=\"powered-by\">{{lang 'footer.powered_by'}}{{moment format=\"YYYY\"}}</p>\n                </div>\n            {{/if}}\n            <article class=\"\"\n                data-section-type=\"footer-webPages\">\n                    {{> components/common/social-links}}\n            </article>\n        </div>\n    </div>\n</footer>\n\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"social-links\">\n        {{#each social_media}}\n            <li>\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<div class=\"footer-container__subscription\">\n    <div class=\"section-container footer-container__subscription-flex\">\n        <h3 class=\"footer-heading\">{{lang 'footer.title'}}</h3>\n        <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n            <fieldset class=\"form-fieldset\">\n                <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n                <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n                <input type=\"hidden\" name=\"check\" value=\"1\">\n                <div class=\"form-field\">\n                    <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n                    <div class=\"form-prefixPostfix\">\n                        <input class=\"form-input\"\n                            id=\"nl_email\"\n                            name=\"nl_email\"\n                            type=\"email\"\n                            value=\"{{customer.email}}\"\n                            placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                            aria-describedby=\"alertBox-message-text\"\n                            aria-required=\"true\"\n                            required\n                        >\n                        <input class=\"button--primary form-prefixPostfix-button--postfix\"\n                            type=\"submit\"\n                            value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                        >\n                    </div>\n                    {{#if settings.show_newsletter_summary }}\n                        <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n                    {{/if}}\n                </div>\n            </fieldset>\n        </form>\n    </div>\n</div>\n"}
