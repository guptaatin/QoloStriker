{"components/account/address-list":"{{#unless customer.addresses}}\n{{> components/common/alert/alert-info (lang 'account.addresses.no_addresses')}}\n{{/unless}}\n\n<ul class=\"addressList\">\n    {{#each customer.addresses}}\n    <li class=\"address\">\n        <div class=\"panel panel--address\">\n            <div class=\"panel-body panel-body-flex\">\n                <h5 class=\"address-company\">{{company}}</h5>\n                <h5 class=\"address-title\">{{first_name}} {{last_name}}</h5>\n                <ul class=\"address-details address-details--postal\">\n                    <li>{{address1}}</li>\n                    <li>{{address2}}</li>\n                    <li>{{city}}{{#if state}}, {{state}}{{/if}} {{zip}}</li>\n                    <li>{{country}}</li>\n                </ul>\n                {{#if phone}}\n                <dl class=\"address-details\">\n                    <dt class=\"address-label\">{{lang 'account.addresses.phone' }}</dt>\n                    <dd class=\"address-description\">{{phone}}</dd>\n                </dl>\n                {{/if}}\n            </div>\n            <form action=\"{{delete_url}}\" method=\"post\" data-delete-address=\"{{lang 'forms.address.confirm_delete'}}\">\n                <div class=\"form-actions\">\n                    <a class=\"adress-link\" href=\"{{edit_url}}\">{{lang 'forms.address.edit.heading' }}</a>\n                    <button type=\"submit\" class=\"adress-link\">{{lang 'forms.address.delete.heading' }}</button>\n                </div>\n            </form>\n        </div>\n    </li>\n    {{/each}}\n</ul>","components/common/alert/alert-info":"<div class=\"alertBox alertBox--info\" data-promotion-banner>\n    <svg data-src='{{cdn \"assets/icons/error.svg\"}}'></svg>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n"}
