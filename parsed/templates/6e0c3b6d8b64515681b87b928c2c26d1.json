{"components/account/orders-list":"<ul class=\"account-list\">\n    {{#each orders}}\n    <li class=\"account-listItem table-container\">\n        <div class=\"account-product\">\n            <div class=\"account-product-body\">\n                <div class=\"order-content table-row\">\n                    <span class=\"account-product-heading\">Order</span>\n                    <span class=\"order-number\">#{{this.id}}</span>\n                </div>\n                <div class=\"order-content table-row\">\n                    <span class=\"account-product-heading\">Placed On</span>\n                    <span>{{this.date}}</span>\n                </div>\n                <div class=\"order-content table-row\">\n                    <span class=\"account-product-heading\">Quantity</span>\n                    <span>{{ this.items.length}} {{#if this.items.length '===' 1}}item{{else}}items{{/if}}</span>\n                </div>\n                <div class=\"order-content table-row\">\n                    <span class=\"account-product-heading\">Total</span>\n                    <span style=\"font-size: 16px;\">{{this.total.formatted}}</span>\n                </div>\n                <div class=\"order-content order-status table-row\">\n                    <h6 class=\"account-orderStatus-label\">{{this.status}}</h6>\n                    {{#if ../settings.returns_enabled}}\n                        {{#if this.return_url}}\n                            <a class=\"account-orderStatus-action\" href=\"{{this.return_url}}\">\n                                {{lang 'account.orders.return_items' }}\n                            </a>\n                        {{/if}}\n                    {{/if}}\n                </div>\n                <div class=\"order-content table-row\">\n                    <a class=\"button\" href=\"{{this.details_url}}\">View Order Details</a>\n                </div>\n            </div>\n        </div>\n    </li>\n    {{/each}}\n</ul>\n"}
