{"components/account/wishlist-list":"{{#if customer.wishlists}}\n<div class=\"favorite-header\">\n    <h5>{{lang 'account.wishlists.title'}}</h5>\n    <div class=\"form-actions\">\n        <a href=\"{{this.urls.account.wishlists.add}}\" class=\"button button--primary\" role=\"button\" data-wishlist>{{lang\n            'account.wishlists.new'}}</a>\n    </div>\n</div>\n<table class=\"table table--line wishlists-table\">\n    <thead class=\"table-thead\">\n        <tr>\n            <th>{{lang 'account.wishlists.name'}}</th>\n            <th>{{lang 'account.wishlists.num_items'}}</th>\n            <th style=\"display: none;\">{{lang 'account.wishlists.action'}}</th>\n        </tr>\n    </thead>\n    <tbody class=\"table-tbody\">\n        {{#each customer.wishlists}}\n        <tr>\n            <td class=\"wishlist-name\"><a href=\"{{view_url}}\">{{{name}}}</a></td>\n            <td>{{num_items}} {{#if num_items \"===\" 1}}item{{else}}items{{/if}}</td>\n            <td class=\"table-actions\">\n                <form method=\"post\" action=\"{{delete_url}}\" class=\"form\">\n                    <fieldset class=\"form-fieldset\">\n                        <a href=\"{{edit_url}}\" class=\"simple-button\" data-wishlist>{{lang\n                            'account.wishlists.edit_name'}}</a>\n                        <input type=\"submit\" value=\"Delete List\" class=\"simple-button\" data-wishlist-delete>\n                    </fieldset>\n                </form>\n            </td>\n        </tr>\n        {{/each}}\n    </tbody>\n</table>\n<div class=\"block-wishlist-mobile\">\n    {{#each customer.wishlists}}\n    <div class=\"block-wishlist\">\n        <div class=\"block-wishlist-top\">\n            <a href=\"{{view_url}}\" class=\"wishlist-name\">{{{name}}}</a>\n            <span>{{num_items}} {{#if num_items \"===\" 1}}item{{else}}items{{/if}}</span>\n        </div>\n        <div class=\"table-actions\">\n            <form method=\"post\" action=\"{{delete_url}}\" class=\"form\">\n                <fieldset class=\"form-fieldset\">\n                    <a href=\"{{edit_url}}\" class=\"simple-button\" data-wishlist>{{lang\n                        'account.wishlists.edit_name'}}</a>\n                    <input type=\"submit\" value=\"Delete List\" class=\"simple-button\" data-wishlist-delete>\n                </fieldset>\n            </form>\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{else}}\n<div class=\"empty-wishlist\">\n    <h6>{{lang 'account.wishlists.no_lists'}}</h6>\n    <p>{{lang 'account.wishlists.wishlist_start'}}</p>\n    <a href=\"{{this.urls.account.wishlists.add}}\">{{lang 'account.wishlists.new'}}</a>\n</div>\n{{/if}}"}
